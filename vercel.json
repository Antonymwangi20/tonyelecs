{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key",
    "VITE_API_BASE_URL": "@vite_api_base_url",
    "VITE_GOOGLE_PAY_ENVIRONMENT": "@vite_google_pay_environment",
    "VITE_GOOGLE_PAY_MERCHANT_ID": "@vite_google_pay_merchant_id",
    "VITE_ADMIN_EMAIL": "@vite_admin_email",
    "VITE_ADMIN_PASSWORD": "@vite_admin_password",
    "VITE_MANAGER_EMAIL": "@vite_manager_email",
    "VITE_MANAGER_PASSWORD": "@vite_manager_password"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
